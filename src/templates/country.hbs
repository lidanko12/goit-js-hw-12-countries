{{#each this}}
<article>
    <h2>{{name}}</h2>
        <img src="{{flag}}" alt="Flag of {{name}}" width="320">
    </a>
    <h3>Capital:</h3>
    <p>{{capital}}</p>
    <h3>Population:</h3>
    <p>{{population}}</p>
    <h3>Languages:</h3>
    <ul class="js-languages-list">
        {{#each languages}}
        <li>
            <p>{{this.name}}</p>
        </li>
        {{/each}}
    </ul>
</article>
{{/each}}